    "scripts": {
        "clear-node-modules": "\"./../../Build/robormdir\" node_modules",
        "clear-dist": "\"./../../Build/robormdir\" dist",
        "start": "cross-env NODE_ENV=development webpack-dev-server --hot --env.api=fake",
        "build-front-with-fake-api": "cross-env NODE_ENV=development webpack --env.api=fake",
        "vendor:clean": "rimraf prebuild",
        "vendor:build": "rimraf prebuild & webpack --config webpack.vendor.dll.config.js",
        "lint": "tslint src/**/*.tsx src/**/*.ts tests/**/*.tsx tests/**/*.ts stories/**/*.tsx stories/**/*.ts story-tests/**/*.tsx story-tests/**/*.ts local_modules/**/*.tsx local_modules/**/*.ts typings/**/*.tsx typings/**/*.ts --project tsconfig.json",
        "ci:lint": "tslint --formatters-dir node_modules/tslint-teamcity-reporter/ --format TSHintTeamcity src/**/*.tsx src/**/*.ts tests/**/*.tsx tests/**/*.ts stories/**/*.tsx stories/**/*.ts local_modules/**/*.tsx local_modules/**/*.ts typings/**/*.tsx typings/**/*.ts --project tsconfig.json",
        "stylelint": "stylelint src/**/*.less local_modules/**/*.less --syntax less",
        "ci:stylelint": "stylelint src/**/*.less local_modules/**/*.less --syntax less --custom-formatter node_modules/stylelint-teamcity-formatter",
        "test": "mocha-webpack --opts tests/mocha-webpack.opts tests/**/*.test.tsx tests/**/*.test.ts",
        "karma-test": "karma start --single-run",
        "test:watch": "mocha-webpack -w --opts tests/mocha-webpack.opts tests/**/*.test.tsx tests/**/*.test.ts",
        "ci:test": "mocha-webpack --opts tests/mocha-webpack.opts --reporter mocha-teamcity-reporter tests/**/*.test.tsx tests/**/*.test.ts",
        "browser-test": "node scripts/RunTestCafeTests.js",
        "ci:browser-test": "node scripts/RunTestCafeTests.teamcity.js",
        "storybook": "cross-env NODE_ENV=development start-storybook -p 6006 -s .",
        "testing-storybook": "cross-env NODE_ENV=development start-storybook -p 6006 -c .testing-storybook",
        "test-mutators:watch": "cross-env NODE_ENV=test webpack --watch --config webpack.tests-mutators.config.js",
        "test-mutators:build": "webpack --config webpack.tests-mutators.config.js"
    },
